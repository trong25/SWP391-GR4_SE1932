<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>DashboardAcademicStaffServlet</servlet-name>
        <servlet-class>controller.headteacher.DashboardAcademicStaffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>controller.headteacher.StudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeadteacherServlet</servlet-name>
        <servlet-class>.DashBoardHeadTeacherServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DirectorDashBoardServlet</servlet-name>
        <servlet-class>Controller.director.DirectorDashBoardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassServlet</servlet-name>
        <servlet-class>Controller.director.ClassServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controllerr.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReviewClassServlet</servlet-name>
        <servlet-class>Controller.director.ReviewClassServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewTimeTableClassServlet</servlet-name>
        <servlet-class>Controller.director.ViewTimeTableClassServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DashboardAcademicStaffServlet</servlet-name>
        <url-pattern>/academicstaff/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentServlet</servlet-name>
        <url-pattern>/academicstaff/student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HeadteacherServlet</servlet-name>
        <url-pattern>/headteacher/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassServlet</servlet-name>
        <url-pattern>/director/class</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DirectorDashBoardServlet</servlet-name>
        <url-pattern>/director/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReviewClassServlet</servlet-name>
        <url-pattern>/director/reviewclass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewTimeTableClassServlet</servlet-name>
        <url-pattern>/director/viewtimetableclass</url-pattern>
    </servlet-mapping>
</web-app>
