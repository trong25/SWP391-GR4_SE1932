<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>DashboardAcademicStaffServlet</servlet-name>
        <servlet-class>Controller.academicstaff.DashboardAcademicStaffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>Controller.academicstaff.StudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controllerr.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardStudentStaffServlet</servlet-name>
        <servlet-class>Controller.student.DashboardStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studentAttendance</servlet-name>
        <servlet-class>model.student.studentAttendance</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DashboardAcademicStaffServlet</servlet-name>
        <url-pattern>/academicstaff/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentServlet</servlet-name>
        <url-pattern>/academicstaff/student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studentAttendance</servlet-name>
        <url-pattern>/studentAttendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboarStudentServlet</servlet-name>
        <url-pattern>/student/dashboard</url-pattern>
    </servlet-mapping>
</web-app>
